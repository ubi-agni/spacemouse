<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- 
  Put this file into /etc/hal/fdi/policy/ to disable spacemouse as x11 input
  http://spacenav.sourceforge.net/faq.html#faq11
-->
<deviceinfo version="0.2">
    <device>
        <match key="info.product" contains="3Dconnexion">
            <remove key="input.x11_driver"/>
        </match>
    </device>
</deviceinfo>
